{
    "env":{
        "browser": true,
        "es2022": true,
        "node": true
    },
    "extends": ["react-app", 
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi":["error", "always"],
        "quotes": ["error", "double"],
        "no-console":["off"]
    }
}